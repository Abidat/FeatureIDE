<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.ovgu.featureide.ui.ahead.editors.ConfigurationEditor"
            default="true"
            extensions="equation, expression"
            icon="icons/EquationIcon.png"
            id="de.ovgu.featureide.ui.editors.ConfigurationEditor"
            name="Configuration Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.ovgu.featureide.ui.ahead.editors.JakEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            default="true"
            extensions="jak"
            icon="icons/JakFileIcon.png"
            id="de.ovgu.featureide.ui.editors.JakEditor"
            name="Jak Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="de.ovgu.featureide.ui.category"
            class="de.ovgu.featureide.ui.ahead.wizards.NewJakFileWizard"
            icon="icons/NewJakFileIcon.png"
            id="de.ovgu.featureide.ui.ahead.wizards.NewJakFileWizard"
            name="Jak File"/>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="de.ovgu.featureide.ui.ahead.properties.BuildPathPropertyPage"
            id="de.ovgu.featureide.ui.ahead.properties.BuildPathPropertyPage"
            name="Java Build Path">
         <enabledWhen>
            <instanceof
                  value="org.eclipse.core.resources.IProject">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="FeatureIDE"
            class="de.ovgu.featureide.ui.ahead.views.collaboration.CollaborationView"
            icon="icons/FeatureIconSmall.ico"
            id="de.ovgu.featureide.ui.ahead.views.collaboration.Collaboration"
            name="Collaboration Diagram"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.ovgu.featureide.ui.ahead.perspective.PerspectiveFactory"
            icon="icons/FeatureIconSmall.ico"
            id="de.ovgu.featureide.ui.FeatureIDEperspective"
            name="FeatureIDE">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="de.ovgu.featureide.ui.ahead.NewFiles"
            label="FeatureIDE"
            visible="false">
         <action
               class="de.ovgu.featureide.ui.ahead.actions.StartEquationFileWizard"
               icon="icons/NewEquationIcon.png"
               id="de.ovgu.featureide.ui.NewEquationFile"
               label="New Equation File"
               style="push"
               toolbarPath="&quot;Normal&quot;">
         </action>
         <action
               class="de.ovgu.featureide.ui.ahead.actions.StartJakFileWizard"
               icon="icons/NewJakFileIcon.png"
               id="de.ovgu.featureide.ui.ahead.NewJakFile"
               label="New Jak File"
               style="push"
               toolbarPath="&quot;Normal&quot;">
         </action>
      </actionSet>
   </extension>
</plugin>
