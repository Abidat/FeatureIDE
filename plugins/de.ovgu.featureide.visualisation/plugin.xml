<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:de.ovgu.featureide.fm.ui.menus.popupMenu?after=de.ovgu.featureide.fm.ui.menus.popupMenu.submenus">
         <command
               commandId="de.ovgu.featureide.visualisation.command.cooccurrence"
               label="Show Feature Co-occurrence"
               style="push">
         </command>
         <command
               commandId="de.ovgu.featureide.visualisation.command.featurerelations"
               label="Show Feature Relations Graph"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.ovgu.featureide.visualisation.ShowFeatureCoocurrenceCommandHandler"
            id="de.ovgu.featureide.visualisation.command.cooccurrence"
            name="Show Feature Co-occurrence">
      </command>
      <command
            defaultHandler="de.ovgu.featureide.visualisation.ShowFeatureRelationsGraphCommandHandler"
            id="de.ovgu.featureide.visualisation.command.featurerelations"
            name="Show Feature Relations Graph">
      </command>
   </extension>

</plugin>
