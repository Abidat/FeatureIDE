<?xml version="1.0" encoding="UTF-8" standalone="no"?>
	<featureModel chosenLayoutAlgorithm="1">
		<struct>
			<and mandatory="true" name="StringMatcher">
				<feature hidden="true" name="Contains"/>
				<and mandatory="true" name="Comparison">
					<feature name="Length"/>
					<and name="Substring_a_b">
						<feature name="Prefix_a_b"/>
					</and>
					<and name="Substring_b_a">
						<feature name="Prefix_b_a"/>
					</and>
					<feature name="Equals"/>
				</and>
			</and>
		</struct>
		<constraints>
			<rule>
				<imp>
					<conj>
						<var>Prefix_a_b</var>
						<var>Prefix_b_a</var>
					</conj>
					<var>Equals</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<conj>
						<var>Length</var>
						<disj>
							<var>Substring_a_b</var>
							<var>Substring_b_a</var>
						</disj>
					</conj>
					<var>Equals</var>
				</imp>
			</rule>
			<rule>
				<eq>
					<var>Contains</var>
					<disj>
						<var>Substring_a_b</var>
						<var>Substring_b_a</var>
					</disj>
				</eq>
			</rule>
			<rule>
				<imp>
					<var>Substring_a_b</var>
					<not>
						<var>Substring_b_a</var>
					</not>
				</imp>
			</rule>
		</constraints>
		<comments/>
		<featureOrder userDefined="false"/>
	</featureModel>
