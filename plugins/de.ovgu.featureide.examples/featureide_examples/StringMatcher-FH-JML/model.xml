<?xml version="1.0" encoding="UTF-8" standalone="no"?>
	<featureModel chosenLayoutAlgorithm="1">
		<struct>
			<and mandatory="true" name="StringMatcher">
				<feature hidden="true" name="Contains"/>
				<alt mandatory="true" name="Comparison">
					<feature mandatory="true" name="Length"/>
					<feature mandatory="true" name="Equals"/>
					<alt abstract="true" mandatory="true" name="substring">
						<and mandatory="true" name="Substring_a_b">
							<feature name="Prefix_a_b"/>
						</and>
						<and mandatory="true" name="Substring_b_a">
							<feature name="Prefix_b_a"/>
						</and>
					</alt>
				</alt>
			</and>
		</struct>
		<constraints>
			<rule>
				<eq>
					<var>Contains</var>
					<disj>
						<var>Substring_a_b</var>
						<var>Substring_b_a</var>
					</disj>
				</eq>
			</rule>
		</constraints>
		<comments/>
		<featureOrder userDefined="false"/>
	</featureModel>
