get-view "Package Explorer" | get-tree | select "HelloWorld-FH-Java/model.xml" | double-click
with [get-editor "HelloWorld-FH-Java Model"] {
    with [get-diagram | get-edit-part -className ModelEditPart | get-edit-part -text Feature] {
        mouse-move 20 15 button1 -height 21 -width 50
        mouse-press 20 15 button1 -height 21 -width 50
        mouse-release 20 15 button1 524288 -height 21 -width 50
        mouse-hover 20 15 -height 21 -width 50
        mouse-move 24 13 button1 -height 21 -width 50
        mouse-press 24 13 button1 -height 21 -width 50
        mouse-release 24 13 button1 524288 -height 21 -width 50
        mouse-hover 24 13 -height 21 -width 50
        activate-direct-edit
    }
    with [get-editbox] {
        set-text X
        key-type Enter
    }
    with [get-diagram] {
        commit-direct-edit
        key-type "M1+s"
    }
}
get-view "Package Explorer" | get-tree | select "HelloWorld-FH-Java" | key-type Del
with [get-window "Delete Resources"] {
    get-button "Delete project contents on disk (cannot be undone)" | check
    get-button OK | click
}
get-view "Package Explorer" | get-tree | get-menu "New/Example..." | click
get-window "New Example" | get-tree | select "FeatureIDE/FeatureIDE Examples" | double-click
with [get-window "FeatureIDE Example Import"] {
    get-tree | select FeatureHouse | get-item "FeatureHouse/HelloWorld-FH-Java" | check
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | select "HelloWorld-FH-Java/model.xml" | double-click
with [get-editor "HelloWorld-FH-Java Model" | get-diagram] {
    with [get-edit-part -className ModelEditPart | get-edit-part -text Feature] {
        mouse-move 12 5 button1 -height 21 -width 50
        mouse-press 12 5 button1 -height 21 -width 50
        mouse-release 12 5 button1 524288 -height 21 -width 50
    }
    mouse-hover 45 221 -height 586 -width 400
}
