get-view "Package Explorer" | get-tree | select "HelloWorld-FH-Java/model.xml" | double-click
with [get-editor "HelloWorld-FH-Java Model"] {
    with [get-diagram] {
        with [get-edit-part -className ModelEditPart | get-edit-part -text Hello] {
            mouse-move 18 8 button1 -height 19 -width 36
            mouse-press 18 8 button1 -height 19 -width 36
            mouse-release 18 8 button1 524288 -height 19 -width 36
        }
        key-type F2
    }
    with [get-editbox] {
        set-text Hi
        key-type Enter
    }
    with [get-diagram] {
        commit-direct-edit
        key-type F2
        get-edit-part -className ModelEditPart | get-edit-part -text Hi | activate-direct-edit
    }
    with [get-editbox] {
        set-text Hello
        key-type Enter
    }
}
