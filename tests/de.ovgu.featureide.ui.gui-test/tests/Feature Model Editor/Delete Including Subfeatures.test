get-view "Package Explorer" | get-tree | select "Elevator-FeatureModeling/model.xml" | double-click
with [get-editor "Elevator-FeatureModeling Model"] {
    with [get-diagram | get-edit-part -className ModelEditPart | get-edit-part -text Safety | get-target-connection 0] {
        with [get-figure 0] {
            mouse-move 5 5 button1 -height 10 -width 10
            mouse-press 5 5 button1 -height 10 -width 10
            mouse-release 5 5 button1 524288 -height 10 -width 10
            mouse-press 5 5 button1 -height 10 -width 10
            mouse-double-click 5 5 button1 -height 10 -width 10
        }
        get-figure 0 | mouse-release 5 5 button1 524288 -height 10 -width 10
    }
    with [get-diagram] {
        with [get-edit-part -className ModelEditPart | get-edit-part -text Permission] {
            mouse-move 33 9 button1 -height 19 -width 62
            mouse-press 33 9 button1 -height 19 -width 62
            mouse-release 33 9 button1 524288 -height 19 -width 62
            mouse-hover 33 9 -height 19 -width 62
            mouse-press 33 9 button3 -height 19 -width 62
            mouse-release 33 9 button3 2097152 -height 19 -width 62
        }
        get-menu "Delete Including Subfeatures (CTRL + D)" | click
    }
    get-diagram | mouse-hover 756 266 -height 689 -width 1142
    get-diagram | key-type "M1+z"
    get-diagram | get-edit-part -className ModelEditPart | get-edit-part -text Permission | activate-direct-edit
    with [get-diagram] {
        commit-direct-edit
        get-edit-part -className ModelEditPart | get-edit-part -text PermissionControl | activate-direct-edit
    }
    with [get-diagram] {
        commit-direct-edit
        get-edit-part -className ModelEditPart | get-edit-part -text FloorPermission | activate-direct-edit
    }
    with [get-diagram] {
        with [get-edit-part -className ModelEditPart | get-edit-part -text Safety | get-target-connection 0 
            | get-figure 0] {
            mouse-move 6 5 button1 -height 10 -width 10
            mouse-press 6 5 button1 -height 10 -width 10
        }
        commit-direct-edit
        with [get-edit-part -className ModelEditPart | get-edit-part -text Safety | get-target-connection 0] {
            with [get-figure 0] {
                mouse-release 6 5 button1 524288 -height 10 -width 10
                mouse-press 6 5 button1 -height 10 -width 10
                mouse-double-click 6 5 button1 -height 10 -width 10
            }
            get-figure 0 | mouse-release 6 5 button1 524288 -height 10 -width 10
        }
    }
    get-diagram | get-edit-part -className ModelEditPart | get-edit-part -text Safety | get-target-connection 0 
        | get-figure 0 | mouse-hover 6 5 -height 10 -width 10
    get-tab-folder | get-tab-item Source | click
