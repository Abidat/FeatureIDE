Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "Elevator-FeatureModeling/model.xml" | double-click
with [get-editor "Elevator-FeatureModeling Model"] {
    with [get-diagram] {
        with [get-edit-part -className ModelEditPart | get-edit-part -text Service] {
            mouse-move 11 9 button1 -height 19 -width 48
            mouse-press 11 9 button1 -height 19 -width 48
            mouse-release 11 9 button1 524288 -height 19 -width 48
        }
        mouse-hover 313 405 -height 689 -width 1142
        key-type Del
    }
    get-diagram | key-type "M1+w"
}
get-window "Save Resource" | get-button No | click
get-view "Package Explorer" | get-tree | select "Elevator-FeatureModeling/model.xml" | double-click
get-editor "Elevator-FeatureModeling Model" | get-tab-folder | get-tab-item Source | click
