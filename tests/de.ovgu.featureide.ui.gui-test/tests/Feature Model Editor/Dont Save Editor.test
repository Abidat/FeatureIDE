get-view "Package Explorer" | get-tree | select "HelloWorld-FH-Java/model.xml" | double-click
with [get-editor "HelloWorld-FH-Java Model"] {
    with [get-diagram] {
        with [get-edit-part -className ModelEditPart | get-edit-part -text Hello] {
            mouse-move 22 15 button1 -height 21 -width 36
            mouse-press 22 15 button1 -height 21 -width 36
            mouse-release 22 15 button1 524288 -height 21 -width 36
            mouse-hover 22 15 -height 21 -width 36
        }
        key-type F2
        get-edit-part -className ModelEditPart | get-edit-part -text Hello | activate-direct-edit
    }
    with [get-editbox] {
        set-text X
        key-type Enter
    }
    with [get-diagram] {
        commit-direct-edit
        get-edit-part -className ModelEditPart | get-edit-part -text X | mouse-hover 15 13 -height 21 -width 19
        mouse-hover 109 103 -height 586 -width 400
    }
}
with [get-editor "HelloWorld-FH-Java Model" | get-diagram] {
    mouse-move 83 332 button1 -height 586 -width 400
    mouse-press 83 332 button1 -height 586 -width 400
    mouse-release 83 332 button1 524288 -height 586 -width 400
}
get-menu "File/Close" | click
get-window "Save Resource" | get-button No | click
get-view "Package Explorer" | get-tree | select "HelloWorld-FH-Java/model.xml" | double-click
with [get-editor "HelloWorld-FH-Java Model" | get-diagram] {
    with [get-edit-part -className ModelEditPart | get-edit-part -text Hello] {
